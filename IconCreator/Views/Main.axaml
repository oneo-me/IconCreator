<View
    x:Class="IconCreator.Views.Main"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:IconCreator.Views">
    <Design.DataContext>
        <local:Main_Model />
    </Design.DataContext>
    <Panel>
        <Image Source="{Binding Image}" />
        <MenuBar Menu="{Binding Menu}" HorizontalAlignment="Right" VerticalAlignment="Top" />
    </Panel>
</View>
